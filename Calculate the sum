#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main(void) {
  int x, y, z, sum;
  sum = 0; //Initialize variable to store the total sum //합계를 저장할 변수 초기화

  printf("Enter 3 integers (x, y, z): "); //Print a message to enter 3 integers //3개의 정수를 입력하라는 메시지 출력 
  scanf("%d %d %d", &x, &y, &z); //Receive three integers as input //3개의 정수를 입력받기

  sum += x; //Add the value of x to the total sum //x값을 합계에 더하기
  sum += y; //Add the value of y to the total sum //y값을 합계에 더하기
  sum += z; //Add the value of z to the total sum //z값을 합계에 더하기

  printf("Sum of three integers: %d\n", sum); //Print the sum of the 3 integers //3개의 정수의 합계를 출력
  return 0;
}
